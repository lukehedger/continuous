{
  "name": "continuous",
  "private": true,
  "scripts": {
    "build": "rollup --config",
    "ci": "start-server-and-test start http://localhost:5000 test:e2e",
    "fmt": "prettier app/**/*.tsx --check",
    "lint": "eslint app/**/*.tsx",
    "start": "sirv public --single",
    "test:e2e": "cypress run --record false",
    "test:unit": "jest app/"
  },
  "dependencies": {
    "react": "16.11.0",
    "react-dom": "16.11.0"
  },
  "devDependencies": {
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "cypress": "3.6.1",
    "eslint": "6.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.16.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "rollup": "1.26.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-typescript": "1.0.1",
    "sirv-cli": "0.4.5",
    "start-server-and-test": "1.10.6",
    "tslib": "1.10.0",
    "typescript": "3.7.2"
  }
}
